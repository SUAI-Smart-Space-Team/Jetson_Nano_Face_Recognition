{"ast":null,"code":"import dbConnect from '../../utils/dbConnect';\nexport default async function handler(req, res) {\n  const {\n    method\n  } = req;\n  await dbConnect();\n\n  switch (method) {\n    case 'POST':\n      try {\n        if (req.body.values.login === process.env.CLOUD_NAME && req.body.values.password === process.env.API_KEY) {\n          res.status(201).json({\n            success: true,\n            data: 'Successful'\n          });\n        } else {\n          res.status(201).json({\n            success: true,\n            data: 'Login is not valid'\n          });\n        }\n      } catch (error) {\n        res.status(400).json({\n          success: false\n        });\n      }\n\n      break;\n\n    default:\n      res.status(400).json({\n        success: false\n      });\n      break;\n  }\n}","map":null,"metadata":{},"sourceType":"module"}